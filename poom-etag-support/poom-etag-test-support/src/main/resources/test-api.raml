#%RAML 1.0
title: Test Api
version: 1
baseUri: https://api.codingmatters.org/test
mediaType: application/json

annotationTypes: !include https://raw.githubusercontent.com/flexiooss/poom-api-specs/1.7.3/apis/core/annotation-types/codingmatters.raml


/:
  displayName: resource
  get:
    (conforms-to): ["org.codingmatters.poom.etag.api.ETaggedRequest"]
    is: [entityRetrieve: {entityType: object, error: Error}, tagged: {}]

types:
  Message: !include https://raw.githubusercontent.com/flexiooss/poom-api-specs/1.7.3/apis/core/types/message.raml
  Error: !include https://raw.githubusercontent.com/flexiooss/poom-api-specs/1.7.3/apis/core/types/error-with-messages.raml

traits:
  tagged: !include https://raw.githubusercontent.com/flexiooss/poom-api-specs/1.7.3/apis/core/traits/entity/tagged.raml
  entityRetrieve: !include https://raw.githubusercontent.com/flexiooss/poom-api-specs/1.7.3/apis/core/traits/entity/retrieve.raml
